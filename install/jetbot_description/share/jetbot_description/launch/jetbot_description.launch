<?xml version="1.0" encoding="UTF-8" ?>
<launch>
    <arg name="laser_visualize" default="false"/>
    <arg name="camera_visualize" default="true"/>
    <arg name="odomsource" default="encoder"/>
    <arg name="camera_xyz"  default="0.065   0      0.0857"/>
    <arg name="camera_rpy"  default="0    0.25  0"/>
    
    <arg name="urdf_file" default="xacro '$(find-pkg-share jetbot_description)/urdf/jetbot.urdf' laser_visualize:=$(var laser_visualize) camera_visualize:=$(var camera_visualize) camera_xyz:='$(var camera_xyz)' camera_rpy:='$(var camera_rpy)' odomsource:='$(var odomsource)' " />

    <node pkg="robot_state_publisher" exec="robot_state_publisher" name="robot_state_publisher">
        <!-- param name="robot_description" from="$(var urdf_file)" / -->
        <param name="robot_description" value="$(command '$(var urdf_file)')" />
        <!-- param name="publish_frequency" type="double" value="50.0" / -->
        <param name="publish_frequency" type="float" value="50.0" />
    </node>
</launch>
